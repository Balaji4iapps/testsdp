{
  "title": "Edit Employee",
  "description": "Updates single Employee record",
  "variables": {
    "employee": {
      "type": "getEmployeeResponse"
    },
    "employeeETag": {
      "type": "string"
    },
    "employeeId": {
      "type": "string",
      "required": true,
      "input": "fromUrl"
    },
    "jobListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Job",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "getallJobResponse",
        "uriParameters": {}
      }
    },
    "saveEmployeeChainInProgress": {
      "type": "boolean",
      "defaultValue": false
    }
  },
  "metadata": {},
  "types": {
    "@decorators.getallJobResponse": {
      "endpoint": "getall_Job",
      "service": "businessObjects",
      "path": "response"
    },
    "@decorators.getEmployeeResponse": {
      "endpoint": "get_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "businessObjectsServiceErrorResponseType": {
      "body": {
        "status": "string",
        "title": "string",
        "o:errorDetails": [
          {
            "detail": "string",
            "o:errorCode": "string"
          }
        ]
      },
      "status": "number"
    },
    "getallJobResponse": {
      "items": [
        {
          "id": "number",
          "jobTitle": "string"
        }
      ]
    },
    "getEmployeeResponse": {
      "email": "string",
      "firstName": "string",
      "hireDate": "string",
      "job": "number",
      "lastName": "string",
      "salary": "number"
    }
  },
  "eventListeners": {
    "backButtonClicked": {
      "chains": [
        {
          "chain": "goBackChain"
        }
      ]
    },
    "saveButtonClicked": {
      "chains": [
        {
          "chain": "saveEmployeeChain"
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "chain": "loadEmployeeChain"
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-date": {
        "path": "ojs/ojdatetimepicker"
      },
      "oj-input-number": {
        "path": "ojs/ojinputnumber"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-select-single": {
        "path": "ojs/ojselectsingle"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      },
      "oj-validation-group": {
        "path": "ojs/ojvalidationgroup"
      }
    },
    "modules": {
      "ojValidationBase": {
        "path": "ojs/ojvalidation-base"
      }
    }
  }
}
